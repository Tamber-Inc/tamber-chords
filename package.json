{
  "name": "tamber-chords",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun run build:js && bun run build:types",
    "build:js": "bun build ./src/index.ts --outdir ./dist --target node",
    "build:types": "tsc -p tsconfig.build.json",
    "build:fuzz": "bun build ./src/index.ts --outdir ./dist-fuzz --target node --format cjs",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "fuzz": "bun run build:fuzz && npx jazzer",
    "fuzz:parseChord": "bun run build:fuzz && npx jazzer fuzz/parseChord.fuzz.js",
    "fuzz:buildChord": "bun run build:fuzz && npx jazzer fuzz/buildChord.fuzz.js",
    "fuzz:chordPalette": "bun run build:fuzz && npx jazzer fuzz/chordPalette.fuzz.js"
  },
  "devDependencies": {
    "@jazzer.js/core": "^2.1.0",
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "trustedDependencies": [
    "@jazzer.js/fuzzer"
  ]
}
